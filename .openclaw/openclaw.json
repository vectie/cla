{
  "agents": {
    "defaults": {
      "model": {
        "primary": "embrace/chatglm"
      },
      "models": {
        "embrace/chatglm": {
          "alias": "Embrace ChatGLM"
        }
      }
    }
  },
  "models": {
    "providers": {
      "embrace": {
        "baseUrl": "http://localhost:8000",
        "api": "openai-completions",
        "models": [
          {
            "id": "chatglm",
            "name": "ChatGLM",
            "reasoning": false,
            "input": ["text"],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      }
    }
  }
}